    "def compute_smd(feature,target):\n",
    "    ds_smd = ds[[feature,target]]\n",
    "\n",
    "    num_mean = ds_smd[feature].mean()\n",
    "    ds_smd[feature + '_SMD'] = (ds_smd[feature].sub(num_mean)).div(num_mean)\n",
    "\n",
    "    smd_summary = ds_smd.groupby([target]).agg({feature + '_SMD': \"mean\", feature: \"mean\"})\n",
    "    return (feature, smd_summary[feature+\"_SMD\"].abs().sum(), smd_summary[feature + '_SMD'].values[0] ,smd_summary[feature + '_SMD'].values[1] )"
   ]
